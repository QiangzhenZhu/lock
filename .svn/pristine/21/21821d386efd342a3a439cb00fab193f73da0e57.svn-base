<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="false">
        <shape>
            <corners android:radius="12dp" />
            <gradient android:angle="270" android:endColor="#5B971B" android:startColor="#9BE764" />
            <stroke android:width="0dp" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="true">
        <layer-list>
            <item>
                <shape>
                    <corners android:radius="12dp" />
                    <gradient android:angle="270" android:endColor="#B8E986" android:startColor="#DDF6BC" />
                    <stroke android:width="0dp" />
                </shape>
            </item>
            <item android:bottom="6dp" android:left="6dp" android:right="6dp" android:top="6dp">
                <shape>
                    <corners android:radius="12dp" />
                    <gradient android:angle="270" android:endColor="#5B971B" android:startColor="#9BE764" />
                    <stroke android:width="0dp" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>